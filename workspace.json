{
  "version": 2,
  "projects": {
    "webportal": {
      "projectType": "application",
      "root": "apps/webportal",
      "sourceRoot": "apps/webportal/src",
      "prefix": "",
      "targets": {
        "build": {
          "executor": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/webportal",
            "index": "apps/webportal/src/index.html",
            "main": "apps/webportal/src/main.ts",
            "polyfills": "apps/webportal/src/polyfills.ts",
            "tsConfig": "apps/webportal/tsconfig.app.json",
            "aot": true,
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/webportal/src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              }
            ],
            "styles": [
              "apps/webportal/src/global.scss",
              "apps/webportal/src/theme/variables.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/webportal/src/environments/environment.ts",
                  "with": "apps/webportal/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "webportal:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "webportal:build:production"
            }
          }
        },
        "extract-i18n": {
          "executor": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "webportal:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/webportal/src/**/*.ts",
              "apps/webportal/src/**/*.html"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/webportal"
          ],
          "options": {
            "jestConfig": "apps/webportal/jest.config.js",
            "passWithNoTests": true
          }
        },
        "ionic-cordova-build": {
          "executor": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            }
          }
        },
        "ionic-cordova-serve": {
          "executor": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "app:ionic-cordova-build",
            "devServerTarget": "app:serve"
          },
          "configurations": {
            "production": {
              "cordovaBuildTarget": "app:ionic-cordova-build:production",
              "devServerTarget": "app:serve:production"
            }
          }
        }
      }
    },
    "webportal-e2e": {
      "root": "apps/webportal-e2e",
      "sourceRoot": "apps/webportal-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/webportal-e2e/cypress.json",
            "tsConfig": "apps/webportal-e2e/tsconfig.e2e.json",
            "devServerTarget": "webportal:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "webportal:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/webportal-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "aqe-util": {
      "projectType": "library",
      "root": "libs/aqe-util",
      "sourceRoot": "libs/aqe-util/src",
      "prefix": "",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "tsConfig": "libs/aqe-util/tsconfig.lib.json",
            "project": "libs/aqe-util/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/aqe-util/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/aqe-util/src/**/*.ts",
              "libs/aqe-util/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": true
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "webportal"
}